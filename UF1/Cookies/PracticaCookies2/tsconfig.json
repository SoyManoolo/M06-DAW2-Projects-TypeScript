{
    "include": ["./src/**/*"],
    "exclude": ["./built"],

    "compilerOptions": {
        "target": "ES2016",
        "module": "commonjs",
        "outDir": "./built",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    }
}