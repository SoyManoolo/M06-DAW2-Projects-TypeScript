{
    "include": ["./src/**/*"],

    "compilerOptions": {
        "target": "ES2022",
        "module": "ES6",
        "outDir": "./dist",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    }
}